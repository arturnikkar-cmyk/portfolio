# 3. –ö–û–ù–§–ï–¢–´
# ---------------------------------------------------------
import random
kommide_arv = random.randint(10, 50)  # —Å–ª—É—á–∞–π–Ω–æ–µ —á–∏—Å–ª–æ –∫–æ–Ω—Ñ–µ—Ç –Ω–∞ —Å—Ç–æ–ª–µ
print("Kommi laual:", kommide_arv)

votab = int(input("Mitu kommi tahad v√µtta? "))
kommide_arv -= votab  # —É–º–µ–Ω—å—à–∞–µ–º –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ –∫–æ–Ω—Ñ–µ—Ç
print("Kommi laual n√º√ºd:", kommide_arv)

# ---------------------------------------------------------
